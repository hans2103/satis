{
    "packages": {
        "magento/magento-cloud-docker": [
            {
                "name": "magento/magento-cloud-docker",
                "version": "1.0.0",
                "version_normalized": "1.0.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://repo.magento.com/archives/magento/magento-cloud-docker/magento-magento-cloud-docker-1.0.0.0.zip",
                    "shasum": "36c834e8c9342bcc3956fb8692ebb95b5739ca17"
                },
                "require": {
                    "composer/composer": "^1.0",
                    "composer/semver": "^1.0",
                    "ext-json": "*",
                    "illuminate/config": "^5.5",
                    "php": "^7.1.3",
                    "symfony/config": "^4.4",
                    "symfony/console": "^2.8||^4.0",
                    "symfony/dependency-injection": "^3.3||^4.3",
                    "symfony/yaml": "^3.3||^4.0"
                },
                "require-dev": {
                    "phpmd/phpmd": "@stable",
                    "phpstan/phpstan": "^0.11",
                    "phpunit/phpunit": "^7.2",
                    "squizlabs/php_codesniffer": "^3.0"
                },
                "bin": [
                    "bin/ece-docker"
                ],
                "type": "magento2-component",
                "extra": {
                    "map": [
                        [
                            "dist/docker-sync.yml",
                            "docker-sync.yml"
                        ],
                        [
                            "dist/mutagen.sh",
                            "mutagen.sh"
                        ],
                        [
                            "dist/mnt",
                            ".docker/mnt"
                        ],
                        [
                            "dist/tmp",
                            ".docker/tmp"
                        ],
                        [
                            "dist/mysql",
                            ".docker/mysql"
                        ],
                        [
                            "dist/bin/magento-docker",
                            "bin/magento-docker"
                        ]
                    ],
                    "chmod": [
                        {
                            "mask": "0755",
                            "path": "bin/magento-docker"
                        },
                        {
                            "mask": "0755",
                            "path": "mutagen.sh"
                        }
                    ]
                },
                "autoload": {
                    "psr-4": {
                        "Magento\\CloudDocker\\": "src/"
                    }
                },
                "license": [
                    "OSL-3.0",
                    "AFL-3.0"
                ],
                "description": "Magento Cloud Docker"
            }
        ]
    }
}
